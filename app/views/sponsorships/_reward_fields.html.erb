<div class="nested-fields">
  <div class="field">
    <br>
    <%= f.number_field :number_of_goods, :class => "form-control", :placeholder => "Number of particular good you will supply (use arrow keys)" %>
  </div>
  <div class="field">
   	<br>
    <%= f.text_field :good, :class => "form-control", :placeholder => "Type of good you will supply" %>
  </div>

  <p><%= link_to_remove_association "remove reward", f, class:'btn btn-danger' %></p>
  
</div>
<br/>
