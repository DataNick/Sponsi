<div class="jumbotron athlete-signin">
<div class="border-form-div">
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>



  <div class="form-group">
    <%= f.label :first_name %> <br />
    <%= f.text_field :first_name, :class => "form-control", :id => "text-comment-name" %>
  </div>

  <div class="form-group">
    <%= f.label :last_name %><br />
    <%= f.text_field :last_name, :class => "form-control", :id => "text-comment-name" %>
  </div>

  <div class="form-group">
    <%= f.label :email %><br />
    <%= f.email_field :email, :class => "form-control", :id => "text-comment-name", autofocus: true %>
  </div>

  <div class="form-group">
    <%= f.label :password %>
    <% if @validatable %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, :class => "form-control", :id => "text-comment-name", autocomplete: "off" %>
  </div>

  <div class="form-group">
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, :class => "form-control", :id => "text-comment-name", autocomplete: "off" %>
  </div>

  <div class="actions">
    <%= f.submit class:'btn btn-success', :value => "Sign up" %>
  </div>
<% end %>
</div>
<br/>
<br/>
</div>